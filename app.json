{
  "expo": {
    "name": "cookie",
    "slug": "cookie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cookie-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/cookie-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asafz.cookie",
      "buildNumber": "1.0.0",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cookie-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.asafz.cookie",
      "versionCode": 1
    },
    "extra": {
      "eas": {
        "projectId": "64319bd5-21a7-4b8b-ba82-58b357d33b6c"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ]
  }
}
